    </rdfs:Class>

    <rdfs:Class rdf:about="https://a11y-test.github.io/vocab/earl2#Format">
        <rdfs:label>Format</rdfs:label>
        <rdfs:comment>Indicates the supported formats. Most evaluation tools check the accessibility of HTML content. 
            Some check other web technologies, such as WAI-ARIA, CSS, SVG, and PDF.**</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="https://a11y-test.github.io/vocab/earl2#"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="https://a11y-test.github.io/vocab/earl2#TypeTool">
        <rdfs:label>TypeTool</rdfs:label>
        <rdfs:comment>Indicates the specific type of the tool, according to how it is used or installed.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="https://a11y-test.github.io/vocab/earl2#"/>
    </rdfs:Class>


